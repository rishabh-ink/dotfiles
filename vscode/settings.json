{
    "[markdown]": {
        "editor.wordWrap": "off"
    },
    "editor.autoIndent": "none",
    "editor.cursorBlinking": "phase",
    "editor.cursorWidth": 3,
    "editor.dragAndDrop": false,
    // "editor.fontFamily": "mplusNerdFontCompleteM-regular",
    // "editor.fontFamily": "monaco",
    // "editor.fontFamily": "FixedsysExcelsiorIIIb",
    "editor.fontFamily": "Iosevka-Medium",
    "terminal.integrated.fontSize": 14,
    "editor.fontSize": 16,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        120,
    ],
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "off",
    "eslint.packageManager": "yarn",
    "eslint.alwaysShowStatus": true,
    "explorer.openEditors.visible": 0,
    "files.exclude": {
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/.DS_Store": true,
        "**/.eggs": true,
        "**/.git": false,
        "**/.hg": true,
        "**/.svn": true,
        "**/.tox": true,
        "**/__pycache__": true,
        "**/build": true,
        "**/CVS": true,
        "**/dist": false,
        "**/node_modules": false,
        "**/templates/*.py": true,
        "**/venv*": true,
        "**/virtualenv_run": true
    },
    "files.trimTrailingWhitespace": true,
    "flow.useNPMPackagedFlow": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "gitlens.codeLens.scopes": [
        "blocks",
        "containers",
        "document"
    ],
    "gitlens.keymap": "alternate",
    "gitlens.statusBar.format": "${author} • ${id} • ${message} • ${ago}",
    "javascript.validate.enable": false,
    "python.formatting.autopep8Path": "/usr/local/bin/autopep8",
    "python.linting.flake8Enabled": false,
    "python.linting.flake8Path": "/usr/local/bin/flake8",
    "python.linting.pylintEnabled": false,
    "window.title": "${dirty} ${activeEditorLong}",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Material Theme Palenight High Contrast",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "none",
    "explorer.confirmDragAndDrop": false,
    "sourcegraph.url": "https://sourcegraph.yelpcorp.com",
    "gitlens.codeLens.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.accessibilitySupport": "off",
    "zenMode.centerLayout": false,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
}