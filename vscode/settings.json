{
    "[markdown]": {
        "editor.wordWrap": "off"
    },
    "editor.autoIndent": "none",
    "editor.cursorBlinking": "phase",
    "editor.cursorWidth": 3,
    "editor.dragAndDrop": false,
    // "editor.fontFamily": "InputMonoCondensed-Light",
    // "editor.fontFamily": "mplusNerdFontCompleteM-regular",
    // "editor.fontFamily": "monaco",
    // "editor.fontFamily": "FixedsysExcelsiorIIIb",
    "editor.fontFamily": "JetBrainsMonoNerdFontCompleteM-Regular",
    "editor.fontSize": 14,
    "editor.fontLigatures": false,
    "terminal.integrated.fontSize": 14,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        120,
    ],
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "off",
    "eslint.packageManager": "yarn",
    "eslint.alwaysShowStatus": true,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true
    },
    "explorer.openEditors.visible": 0,
    "files.exclude": {
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/.DS_Store": true,
        "**/.eggs": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/.tox": true,
        "**/__pycache__": true,
        "**/build": false,
        "**/CVS": true,
        "**/dist": false,
        "**/node_modules": true,
        "**/templates/*.py": true,
        "**/venv*": true,
        "**/virtualenv_run": true
    },
    "files.trimTrailingWhitespace": true,
    "flow.useNPMPackagedFlow": true,
    "javascript.validate.enable": false,
    "python.formatting.autopep8Path": "/usr/local/bin/autopep8",
    "python.linting.flake8Enabled": false,
    "python.linting.flake8Path": "./virtualenv_run/bin/flake8",
    "python.linting.pylintEnabled": false,
    "window.title": "${dirty} ${activeEditorLong}",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Material Theme Palenight High Contrast",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "none",
    "explorer.confirmDragAndDrop": false,
    "sourcegraph.url": "https://sourcegraph.yelpcorp.com",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.accessibilitySupport": "off",
    "zenMode.centerLayout": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": false,
    },
    "editor.formatOnSave": false,
    "explorer.compactFolders": false,
    "materialTheme.accent": "Orange",
    "eslint.format.enable": true,
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.currentLine.enabled": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.suggestSelection": "first",
    "python.jediEnabled": false,
    "python.languageServer": "Microsoft",
    "workbench.fontAliasing": "auto",
}