{
    "[markdown]": {
        "editor.wordWrap": "off"
    },
    "css.validate": false,
    "editor.autoIndent": "none",
    "editor.cursorBlinking": "phase",
    "editor.cursorWidth": 3,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "InputMonoCondensed-Medium",
    // "editor.fontFamily": "mplusNerdFontCompleteM-medium",
    // // "editor.fontFamily": "monaco",
    // "editor.fontFamily": "FixedsysExcelsiorIIIb",
    // "editor.fontFamily": "JetBrainsMonoNerdFontCompleteM-Bold",
    // "editor.fontFamily": "JetBrainsMonoMediumNerdFontCompleteM-Medium",
    // "editor.fontFamily": "IosevkaNerdFontCompleteM-",
    "editor.fontLigatures": false,
    "terminal.integrated.fontSize": 12,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        120,
    ],
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "off",
    "eslint.packageManager": "yarn",
    "eslint.alwaysShowStatus": true,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true
    },
    "explorer.openEditors.visible": 0,
    "files.exclude": {
        "**/*.egg-info": true,
        "**/*.pyc": true,
        "**/.DS_Store": true,
        "**/.eggs": true,
        "**/.git": false,
        "**/.hg": true,
        "**/.svn": true,
        "**/.tox": true,
        "**/build": false,
        "**/CVS": true,
        "**/dist": false,
        "**/node_modules": true,
        "**/templates/*.py": true,
        "**/venv*": true,
        "**/virtualenv_run": true,
        "**/bazel*/**": true,
        "**/*cache*": true,
        "**/lib/**": false,
        "**/.netlify/**": true,
        "**/.next/**": true,
        "**/.vscode/**": true,
        "**/.npm-prefix/**": true,
        "**/coverage/**": true,
        "**/out/**": true,
    },
    "files.trimTrailingWhitespace": true,
    "flow.useNPMPackagedFlow": true,
    "javascript.validate.enable": false,
    "python.formatting.autopep8Path": "/usr/local/bin/autopep8",
    "python.linting.flake8Enabled": false,
    "python.linting.flake8Path": "./virtualenv_run/bin/flake8",
    "python.linting.pylintEnabled": false,
    "window.title": "${dirty} ${activeEditorLong}",
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Material Theme Palenight High Contrast",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "none",
    "explorer.confirmDragAndDrop": false,
    "sourcegraph.url": "https://sourcegraph.yelpcorp.com",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.accessibilitySupport": "off",
    "zenMode.centerLayout": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "editor.formatOnSave": false,
    "explorer.compactFolders": false,
    "materialTheme.accent": "Orange",
    "eslint.format.enable": true,
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.currentLine.enabled": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.suggest.paths": false,
    "path-intellisense.mappings": {
        "<PROJECT_ROOT>": "${workspaceRoot}",
    },
    "editor.suggestSelection": "first",
    "python.jediEnabled": false,
    "python.languageServer": "Pylance",
    "gitlens.blame.highlight.locations": [
        "gutter",
        "line"
    ],
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.reduceFlicker": true,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.indent": 12,
    "[typescriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "editor.fontSize": 14,
    "workbench.fontAliasing": "antialiased",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
}