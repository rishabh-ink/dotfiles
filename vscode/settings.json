{
    "[markdown]": {
        "editor.wordWrap": "off"
    },
    "css.validate": false,
    "editor.autoIndent": "none",
    "editor.cursorBlinking": "phase",
    "editor.cursorWidth": 3,
    "editor.dragAndDrop": false,
    // "editor.fontFamily": "FantasqueSansMono-Regular",
    "editor.letterSpacing": -1,
    "editor.fontFamily": "InputMonoCondensed-Medium",
    // "editor.fontFamily": "Lotion-Regular",
    // "editor.fontFamily": "VictorMonoNerdFontComplete-Bold",
    // "editor.fontFamily": "ComicShanns-Regular",
    // "editor.fontFamily": "Dossier-Regular",
    // "editor.fontFamily": "mplusNerdFontCompleteM-regular",
    // // "editor.fontFamily": "monaco",
    // "editor.fontFamily": "FixedsysExcelsiorIIIb",
    // "editor.fontFamily": "JetBrainsMonoNerdFontCompleteM-Bold",
    // "editor.fontFamily": "JetBrainsMonoMediumNerdFontCompleteM-Medium",
    // "editor.fontFamily": "IosevkaNerdFontCompleteM-",
    "editor.fontLigatures": true,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontFamily": "mplusNerdFontCompleteM-regular",
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.rulers": [
        120,
    ],
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "off",
    "eslint.packageManager": "yarn",
    "eslint.alwaysShowStatus": true,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true
    },
    "explorer.openEditors.visible": 0,
    "files.exclude": {
        "**/*.egg-info": true,
        "**/*.yarn": true,
        "**/*flow-typed": true,
        "**/*.pyc": true,
        "**/.DS_Store": true,
        "**/.eggs": true,
        "**/.git": false,
        "**/.hg": true,
        "**/.svn": true,
        "**/.tox": false,
        "**/build": false,
        "**/CVS": true,
        "**/dist": false,
        "**/node_modules": false,
        "**/templates/*.py": false,
        "**/*.test.js.snap": true,
        "**/venv*": true,
        "**/virtualenv_run": true,
        "**/bazel*/**": true,
        "**/*cache*": false,
        "**/lib/**": false,
        "**/*.js.flow": true,
        "**/.netlify/**": true,
        "**/.next/**": true,
        "**/.vscode/**": true,
        "**/.npm-prefix/**": true,
        "**/coverage/**": true,
        "**/out/**": true,
    },
    "files.trimTrailingWhitespace": true,
    "flow.useNPMPackagedFlow": true,
    "javascript.validate.enable": false,
    "python.formatting.autopep8Path": "/usr/local/bin/autopep8",
    "python.linting.flake8Enabled": false,
    "python.linting.flake8Path": "./virtualenv_run/bin/flake8",
    "python.linting.pylintEnabled": false,
    "window.title": "${dirty} ${activeEditorLong}",
    "workbench.colorTheme": "Material Theme Palenight High Contrast",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "none",
    "explorer.confirmDragAndDrop": false,
    "sourcegraph.url": "https://sourcegraph.yelpcorp.com",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "editor.accessibilitySupport": "off",
    "zenMode.centerLayout": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "editor.formatOnSave": false,
    "explorer.compactFolders": false,
    "materialTheme.accent": "Orange",
    "eslint.format.enable": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.currentLine.enabled": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.tsdk": "node_modules/typescript/lib",
    "typescript.suggest.paths": false,
    "path-intellisense.mappings": {
        "<PROJECT_ROOT>": "${workspaceRoot}",
    },
    "editor.suggestSelection": "first",
    "python.jediEnabled": false,
    "python.languageServer": "Pylance",
    "gitlens.blame.highlight.locations": [
        "gutter",
        "line"
    ],
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.reduceFlicker": true,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.indent": 12,
    "[typescriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "workbench.fontAliasing": "antialiased",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.renderWhitespace": "none",
    "workbench.editor.untitled.hint": "hidden",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "editor.fontSize": 14,
    "security.workspace.trust.untrustedFiles": "open",
    "diffEditor.renderSideBySide": false,
    "github-enterprise.uri": "https://github.yelpcorp.com",
    "flow.useBundledFlow": false,
    "editor.largeFileOptimizations": false,
    "window.zoomLevel": 1,
}